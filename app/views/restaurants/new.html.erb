<h1>Creer un nouveau restaurant</h1>

<%= form_for(@restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :category_id %>
  <%= f.select :category_id, {'chinese' => 1, 'italian' => 2, 'japanese' => 3, 'french' => 4, 'belgian' => 5 } %>

  <%= f.submit %>
<% end %>

<%= link_to 'Cancel', restaurants_path %>